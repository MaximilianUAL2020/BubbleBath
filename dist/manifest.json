{
  "name": "BubbleBath",
  "version": "1.0.0",
  "description": "Understand how you spend your time while browsing the web.",
  "permissions": ["history", "tabs", "browsingData", "https://*/", "http://*/"],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "js": ["js/content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "manifest_version": 2
}
