{
  "name": "210425_Build_1",
  "version": "1.0.0",
  "description": "Learning to code chrome extensions!",
  "permissions": ["history", "tabs", "browsingData", "https://*/", "http://*/"],
  "content_scripts": [
    {
      "css": ["css/style.css"],
      "js": ["js/content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
